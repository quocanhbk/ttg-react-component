ments\\Modal.js",["204"],"import React, { useState, useEffect } from 'react'\r\nimport styled, { keyframes } from 'styled-components'\r\nimport {getDarker} from '../../utils/color'\r\nimport PropTypes from 'prop-types'\r\nconst StyledModal = styled.div`\r\n    color: ${props => props.theme.color.text.primary};\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    display: ${props => props.visible ? \"block\" : \"none\"};\r\n    transition: opacity 200ms linear;\r\n    opacity: ${props => props.ani ? 1: 0};\r\n`;\r\nconst ExitIcon = styled.div`\r\n    --clr: ${props => props.theme.color.fill.danger};\r\n    left: 100%;\r\n    top: 50%;\r\n    transform: translate(-140%, -50%) rotate(45deg);\r\n    width: 24px;\r\n    height: 24px;\r\n    position: absolute;\r\n    transition: all 0.3s ease-in-out;\r\n    &:hover {\r\n        transform: translate(-140%, -50%) rotate(225deg);\r\n    }\r\n    &::before {\r\n        border-radius: 25%;\r\n        position: absolute;\r\n        content: \"\";\r\n        top: 2px;\r\n        left: 10px;\r\n        width: 4px;\r\n        height: 18px;\r\n        background: var(--clr);\r\n    }\r\n    &::after {\r\n        border-radius: 25%;\r\n        position: absolute;\r\n        content: \"\";\r\n        top: 2px;\r\n        left: 10px;\r\n        width: 4px;\r\n        height: 18px;\r\n        background: var(--clr);\r\n        transform: rotate(90deg);\r\n    }\r\n\r\n`;\r\nconst modalEnter = keyframes`\r\n    from { top: -20%; }\r\n    to { top: 50%; }\r\n`;\r\n\r\nconst grayen = keyframes`\r\n    from { background: rgba(0,0,0,0);}\r\n    to {background: rgba(0,0,0,0.3);}\r\n`;\r\nconst StyledBackground = styled.div`\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    animation: ${grayen} 0.5s ease-out 0s 1 forwards normal;\r\n`;\r\nconst StyledContainer = styled.div`\r\n    border: 1px solid ${props => props.theme.color.border.primary};\r\n    background: ${props => props.theme.color.background.primary};\r\n    border-radius: 8px;\r\n    display: inline-block;\r\n    overflow: hidden;\r\n    position: absolute;\r\n    left: 50%;\r\n    transition: all 500ms linear;\r\n    height: ${props => props.height || \"auto\"};\r\n    width: ${props => props.width || \"auto\"};\r\n    transform: ${props => props.ani ? \"translate(-50%, -50%)\" : \"translate(-50%, 700px)\"};\r\n    animation: ${modalEnter} 0.5s ease-out 0s 1 forwards normal;\r\n    z-index: 2;\r\n    box-shadow: 0px 0px 8px ${props => getDarker(props.theme.color.border.primary)};\r\n`;\r\nconst StyledTitle = styled.h1`\r\n    position: relative;\r\n    border-bottom: 1px solid ${props => props.theme.color.border.primary};\r\n    font-size: 1.4rem;\r\n    padding: 0.5rem 1rem;\r\n    text-align: center;\r\n    box-shadow: 0px 0px 5px ${props => getDarker(props.theme.color.border.primary)};\r\n`;\r\nconst StyledBody = styled.div`\r\n    padding: 8px 16px;\r\n`;\r\nconst Modal = (props) => {\r\n    const [open, setOpen] = useState(props.visible)\r\n    const [runAni, setRunAni] = useState(true)\r\n    useEffect(() => {\r\n        if (!props.visible) {\r\n            setRunAni(false)\r\n            setTimeout(() => {\r\n                setOpen(props.visible)\r\n            }, 200);\r\n        } else {\r\n            setOpen(props.visible)\r\n            setRunAni(true)\r\n        }\r\n\r\n    }, [props.visible])\r\n    useEffect(() => {\r\n        document.addEventListener(\"keydown\", (e) => {\r\n            if (e.key === \"Escape\") {\r\n                props.onClickOutside()\r\n            }\r\n        })\r\n\r\n        return(() => {\r\n            document.removeEventListener(\"keydown\", (e) => {\r\n                if (e.key === \"Escape\") {\r\n                  props.onClickOutside()\r\n                }\r\n            })\r\n        })\r\n      }, [])\r\n    return (\r\n        <StyledModal visible={open} ani={runAni}>\r\n        